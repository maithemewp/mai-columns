{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mai/columns",
  "version": "0.1.0",
  "title": "Mai Columns",
  "category": "design",
  "description": "Repeatable column arrangements.",
  "attributes": {
    "alignItems": {
      "type": "string"
    },
    "justifyContent": {
      "type": "string"
    },
    "sizesLg": {
      "type": "array",
      "default": []
    },
    "sizesMd": {
      "type": "array",
      "default": []
    },
    "sizesSm": {
      "type": "array",
      "default": [
        "1/1"
      ]
    },
    "innerBlocks": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": {
        "sides": [
          "horizontal",
          "vertical"
        ]
      }
    }
  },
  "providesContext": {
    "mai/sizesLg": "sizesLg",
    "mai/sizesMd": "sizesMd",
    "mai/sizesSm": "sizesSm"
  },
  "textdomain": "mai-columns",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}